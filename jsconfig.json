{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "utils/*": [
        "src/utils/*"
      ],
      "constants/*": [
        "src/constants/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "redux/*": [
        "src/redux/*"
      ],
      "api/*": [
        "src/api/*"
      ],
      "styles/*": [
        "src/styles/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}