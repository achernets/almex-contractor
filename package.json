{
  "name": "almex-contractor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.26.6",
    "connected-react-router": "^6.6.1",
    "formik": "^2.1.1",
    "formik-antd": "^1.7.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-redux-i18n": "^1.9.3",
    "react-redux-modals": "^1.1.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.5",
    "redux-async-initial-state": "^0.3.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.1",
    "yup": "^0.28.0"
  },
  "scripts": {
    "start": "EXTEND_ESLINT=true CHOKIDAR_USEPOLLING=true react-scripts start",
    "build": "EXTEND_ESLINT=true CHOKIDAR_USEPOLLING=true react-scripts build",
    "test": "EXTEND_ESLINT=true CHOKIDAR_USEPOLLING=true react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "thrift": "node thrift/index.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "KazFilter": true,
      "FilterFieldType": true,
      "FilterCondition": true,
      "FilterItem": true,
      "MrkClientSession": true,
      "MrkDocumentType": true,
      "MrkDocumentData": true,
      "MrkDocument": true,
      "ContentItemType": true
    },
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "no-console": "warn",
      "no-unused-vars": "warn",
      "no-unexpected-multiline": "warn",
      "quotes": [
        "warn",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ],
      "semi": [
        "warn",
        "always"
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://achernets.github.io/almex-contractor",
  "devDependencies": {
    "child_process": "^1.0.2",
    "concat-files": "^0.1.1",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.0",
    "replace-in-file": "^5.0.2"
  }
}
