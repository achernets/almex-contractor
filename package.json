{
  "name": "almex-contractor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "3.26.9",
    "babel-plugin-import": "^1.13.0",
    "connected-react-router": "^6.7.0",
    "customize-cra": "^0.9.1",
    "formik": "^2.1.4",
    "formik-antd": "^1.8.1",
    "jsrsasign": "^8.0.12",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-redux-i18n": "^1.9.3",
    "react-redux-modals": "^1.1.4",
    "react-render-props-script-loader": "^1.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-stomp": "^5.0.0",
    "react-use": "^13.27.0",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.5",
    "redux-async-initial-state": "^0.3.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.0.1",
    "yup": "^0.28.1"
  },
  "scripts": {
    "start": "EXTEND_ESLINT=true CHOKIDAR_USEPOLLING=true react-app-rewired start",
    "build": "EXTEND_ESLINT=true CHOKIDAR_USEPOLLING=true react-app-rewired build",
    "test": "EXTEND_ESLINT=true CHOKIDAR_USEPOLLING=true react-app-rewired test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "thrift": "node thrift/index.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "KazFilter": true,
      "FilterFieldType": true,
      "FilterCondition": true,
      "FilterItem": true,
      "MrkClientSession": true,
      "MrkDocumentType": true,
      "MrkDocumentData": true,
      "MrkDocument": true,
      "ContentItemType": true,
      "MrkOrganization": true,
      "MrkContactType": true,
      "MrkContactInfo": true,
      "MrkClient": true,
      "MrkAccount": true,
      "AttachmentType": true,
      "DocsAPI": true,
      "SignInSystem": true,
      "PreconditionException": true,
      "MrkDocResponceType": true,
      "MrkDocumentRespStatus": true,
      "UserOrGroup": true,
      "HBColumnType": true
    },
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "no-console": "warn",
      "no-unused-vars": "warn",
      "no-unexpected-multiline": "warn",
      "quotes": [
        "warn",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ],
      "semi": [
        "warn",
        "always"
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://achernets.github.io/almex-contractor",
  "devDependencies": {
    "child_process": "^1.0.2",
    "concat-files": "^0.1.1",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.1",
    "replace-in-file": "^5.0.2"
  }
}